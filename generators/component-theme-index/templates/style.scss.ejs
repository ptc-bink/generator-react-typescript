/** 
 * This is automatically generated component's themes index file.
 * Any changes in this file will be overwrited.
 * Please customise component theme in theme files.    
 * */

/* importing standart theme */
@import './theme';

/* importing themes */
<% themes.forEach(function(theme){ %>
:global(.theme-<%= theme %>) {
    @import './theme-<%= theme %>';
}<% }); %>
